{
    "html":{
        "url":  "https://web.immomo.com/live"
    },
    "type": ["web", "web2", "rank"],
    "rank":{
        "api":  "https://live-api.immomo.com/m/rank/lists?rank=",
        "rankType": ["star_day", "star_potential", "star_hour", "star_week", "user_day", "user_week"]
    },
    "folders": {
        "logdir": "logs"
    },
    "logconfig": {
        "version": 1,
        "disable_existing_loggers": true,
        "formatters": {
            "standard": {
                "format": "%(asctime)s [%(module)s] %(levelname)s: %(message)s",
                "datefmt": "%Y%m%d-%H%M%S%z"
            },
            "mail" : {
            }
        },
        "handlers": {
            "console": {
                "level": "INFO",
                "formatter": "standard",
                "class": "logging.StreamHandler",
                "stream": "ext://sys.stdout"
            },
            "file": {
                "level": "DEBUG",
                "formatter": "standard",
                "class": "logging.FileHandler",
                "filename": "momo-rank.log"
            },
            "gmail": {
                "level": "INFO",
                "class": "general.depot.Mail",
                "formatter": "mail",
                "fromaddr": "rxq.proxy@gmail.com",
                "toaddrs": ["wenyao.zhang@unifr.ch"],
                "subject": "momo-rank",
                "credentials": ["rxq.proxy@gmail.com", "njhpmandskgawjjt"],
                "mailhost": ["smtp.gmail.com", 587]
            }
        },
        "loggers": {
            "mail": {
                "level": "INFO",
                "handlers": ["gmail"],
                "propagate": 0
            },
            "requests": {
                "level": "WARNING"
            }
        },
        "root": {
            "level": "DEBUG",
            "handlers": ["console", "file"]
        }
    }
}
